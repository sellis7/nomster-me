<div class="booyah-box col-xs-10 col-xs-offset-1">
	<h1>Edit the location details</h1>
	<%= simple_form_for @place do |f| %>
		<%= f.input :name %>
		<br />
		<%= f.input :address %>
		<br />
		<%= f.input :description %>
</div>
<div class="col-xs-10 col-xs-offset-1">
	<br />
		<%= f.submit 'Update', :class => 'btn btn-primary btn-lg text-right' %>
	<% end %>
</div>
